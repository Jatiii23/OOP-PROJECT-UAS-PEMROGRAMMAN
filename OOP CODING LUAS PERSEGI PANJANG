#include <iostream>
using namespace std;

class Rectangle {
    int lebar, tinggi;
  public:
    void set_values (int,int);
    int area() {return lebar*tinggi;}
};

void Rectangle::set_values (int x, int y) {
  lebar  = x;
  tinggi = y;
}

int main () {
  Rectangle rect;
  rect.set_values (3,4);
  cout << "area: " << rect.area();
  return 0;
}
