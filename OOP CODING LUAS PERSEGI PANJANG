#include <iostream>
using namespace std;

class PersegiPanjang 
{
    int lebar, tinggi;
   public:
    void set_nilai (int,int);
    int Luas() {return lebar*tinggi;}
};

void PersegiPanjang::set_nilai (int x, int y)
{
  lebar  = x;
  tinggi = y;
}

int main ()
{
  PersegiPanjang persegi;
  persegi.set_nilai (5,9);
  cout << "Luas Persegi Panjang: " << persegi.Luas();
  return 0;
}
